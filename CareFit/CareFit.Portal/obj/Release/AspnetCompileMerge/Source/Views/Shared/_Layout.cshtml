<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <meta name="description" content="">
    <meta name="keywords" content="admin, academia,sistema musculação, carefit, fitness, sistema academia">
    <meta name="author" content="">

    <!-- BOOTSTRAP -->

    <link href="~/Content/Laceng/assets/css/bootstrap.min.css" rel="stylesheet">



    <!-- LANCENG CSS -->
    <link href="~/Content/Laceng/assets/css/style.css" rel="stylesheet">
    <link href="~/Content/Laceng/assets/css/style-responsive.css" rel="stylesheet">

    <!-- VENDOR -->
    <link href="~/Content/Laceng/assets/css/animate.css" rel="stylesheet">
    <link href="~/Content/Laceng/assets/third/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    @*<link href="~/Content/Laceng/assets/third/weather-icon/css/weather-icons.min.css" rel="stylesheet">*@
    <link href="~/Content/Laceng/assets/third/morris/morris.css" rel="stylesheet">
    <link href="~/Content/Laceng/assets/third/nifty-modal/css/component.css" rel="stylesheet">
    <link href="~/Content/Laceng/assets/third/sortable/sortable-theme-bootstrap.css" rel="stylesheet">
    <link href="~/Content/Laceng/assets/third/icheck/skins/minimal/grey.css" rel="stylesheet">
    <link href="~/Content/Laceng/assets/third/select/bootstrap-select.min.css" rel="stylesheet">
    <link href="~/Content/Laceng/assets/third/summernote/summernote.css" rel="stylesheet">
    <link href="~/Content/Laceng/assets/third/magnific-popup/magnific-popup.css" rel="stylesheet">
    <link href="~/Content/Laceng/assets/third/datepicker/css/datepicker.css" rel="stylesheet">

